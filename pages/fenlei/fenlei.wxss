.container{
	background-color: #fff;
	padding-left: 156rpx;
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	box-sizing: border-box;
}
.leftB{
	position: fixed;
	top: 0;
	left: 0;
	bottom: 0;
	width: 156rpx;
	border: 1px solid #dcdcdc;
	background-color: #eeeeee;
}
.rightB{
	width: 100%;
	height: 100%;
	/* padding: 0 52rpx 0 30rpx; */
	padding: 0 40rpx;
	box-sizing: border-box;
	display: flex;
	flex-direction: column;
}
.mt1{
	width: 100%;
	height: 130rpx;
	display: flex;
	justify-content: center;
	align-items: center;
}
.hx{
	width: 30rpx;
	border-bottom: 2rpx solid #bbbbbb;
}
.title{
	
	font-size: 28rpx;
	color: #000;
	padding: 0 40rpx;
}
.sp1{
	width: 100%;
	display: flex;
	flex-wrap: wrap;
	justify-content: space-between;
}
.spone{
	width: 225rpx;
}
.spimg{
	width: 220rpx;
	height: 220rpx;
	border: 1px solid #f0f0f0;
}
.spmsg{
	height: 70rpx;
	padding: 30rpx 0 40rpx;
	display: flex;
	flex-direction: column;
	justify-content: space-between;
}
.spmsg1{
	width: 100%;
	font-size: 28rpx;
	color: #000;
	text-align: center;
	text-overflow:ellipsis;
	 white-space:nowrap; 
	 overflow:hidden;
}
.spmsg2{
	width: 100%;
	font-size: 26rpx;
	color: #fe0042;
	display: flex;
	justify-content: center;
}
.tabbar1{
	width: 136rpx;
	padding-left: 22rpx;
	height: 105rpx;
	line-height: 105rpx;
	font-size: 28rpx;
	color: #333;
	text-overflow:ellipsis;
	white-space:nowrap; 
	overflow:hidden;
}

.tabbar2{
	background-color: #fff;
}
.more{
	margin-bottom: 20px;
	width: 100%;
	height: 80rpx;
	background-color: #fff;
	border-radius: 6rpx;
	color: #333;
	font-size: 28rpx;
	display: flex;
	justify-content: center;
	align-items: center;
}
.loadb{
	font-size: 26rpx;
	color: #999;
	text-align: center;
}